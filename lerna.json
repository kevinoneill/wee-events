{
  "lerna": "2.5.1",
  "version": "0.6.0",
  "npmClient": "yarn",
  "useWorkspaces": false,
  "gitVersionPrefix": "v",
  "packages": [
    "packages/*"
  ],
  "command": {
    "init": {
      "exact": true
    },
    "bootstrap": {
      "hoist": false
    },
    "publish": {
      "exact": true,
      "allowBranch": "master"
    }
  }
}
