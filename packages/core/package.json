{
  "name": "@weegigs/events-core",
  "version": "0.12.5",
  "description": "Core components for the wee gigs event sourcing system",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "author": "Kevin O'Neill <kevin@oneill.id.au>",
  "scripts": {
    "build": "yarn run lint && yarn run compile",
    "prebuild": "yarn run clean",
    "clean": "rimraf lib",
    "compile:tsc": "tsc -p .",
    "compile": "yarn run compile:tsc",
    "lint": "tslint -c tslint.json --project . \"src/**/*.ts\"",
    "test:unit": "jest \"tests/*\" --env=node --coverage --detectOpenHandles --forceExit",
    "test": "yarn run test:unit",
    "watch:tsc": "tsc -p . --watch",
    "watch:test": "jest \"tests/*\" --env=node --watch",
    "watch": "concurrently -k --names 'tsc' 'yarn run watch:tsc'",
    "prepublishOnly": "yarn run clean && yarn run lint && yarn run compile"
  },
  "devDependencies": {
    "@types/async": "2.0.49",
    "@types/faker": "4.1.2",
    "@types/jest": "23.1.0",
    "@types/mz": "0.0.32",
    "@types/node": "10.3.3",
    "@types/ramda": "0.25.33",
    "@types/winston": "2.3.9",
    "concurrently": "^3.5.1",
    "faker": "4.1.0",
    "jest": "23.1.0",
    "jest-cli": "23.1.0",
    "jest-environment-node-debug": "2.0.0",
    "prettier": "1.13.5",
    "rimraf": "2.6.2",
    "rxjs": "6.2.1",
    "rxjs-tslint": "0.1.4",
    "ts-jest": "22.4.6",
    "tslint": "5.10.0",
    "tslint-config-prettier": "1.13.0",
    "tslint-config-standard": "7.0.0",
    "typescript": "2.9.2",
    "winston": "2.4.2"
  },
  "dependencies": {
    "async": "2.6.1",
    "immutable": "4.0.0-rc.7",
    "moment": "2.22.2",
    "mz": "2.7.0",
    "ramda": "0.25.0",
    "tsmonad": "0.8.0",
    "ulid": "2.3.0"
  },
  "peerDependencies": {
    "rxjs": "^6.1.0",
    "winston": "^2.4.0"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "./node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts)$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  },
  "engines": {
    "node": ">=8.9"
  },
  "files": [
    "lib",
    "package.json",
    "README.md",
    "CHANGELOG.md",
    "LICENCE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kevinoneill/wee-events.git"
  },
  "publishConfig": {
    "access": "public"
  }
}
