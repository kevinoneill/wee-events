{
  "name": "@weegigs/events-core",
  "version": "0.9.1",
  "description": "Core components for the wee gigs event sourcing system",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "author": "Kevin O'Neill <kevin@oneill.id.au>",
  "scripts": {
    "build": "yarn run lint && yarn run compile",
    "prebuild": "yarn run clean",
    "clean": "rimraf lib",
    "compile:tsc": "tsc -p .",
    "compile": "yarn run compile:tsc",
    "lint": "tslint -c tslint.json --project . \"src/**/*.ts\"",
    "test:unit": "jest \"tests/*\" --env=node --coverage --forceExit",
    "test": "yarn run test:unit",
    "watch:tsc": "tsc -p . --watch",
    "watch:test": "jest \"tests/*\" --env=node --watch",
    "watch": "concurrently -k --names 'tsc' 'yarn run watch:tsc'",
    "prepublishOnly": "yarn run clean && yarn run lint && yarn run compile"
  },
  "devDependencies": {
    "@types/async": "2.0.47",
    "@types/faker": "4.1.2",
    "@types/jest": "22.2.0",
    "@types/mz": "0.0.32",
    "@types/node": "9.4.7",
    "@types/ramda": "0.25.20",
    "@types/winston": "2.3.8",
    "concurrently": "^3.5.1",
    "faker": "4.1.0",
    "jest": "22.4.2",
    "jest-cli": "22.4.2",
    "jest-environment-node-debug": "2.0.0",
    "prettier": "1.11.1",
    "rimraf": "2.6.2",
    "rxjs": "5.5.7",
    "ts-jest": "22.4.1",
    "tslint": "5.9.1",
    "tslint-config-prettier": "1.10.0",
    "tslint-config-standard": "7.0.0",
    "typescript": "2.7.2",
    "winston": "2.4.1"
  },
  "dependencies": {
    "async": "2.6.0",
    "immutable": "4.0.0-rc.7",
    "moment": "2.21.0",
    "mz": "2.7.0",
    "ramda": "0.25.0",
    "tsmonad": "0.8.0",
    "ulid": "2.3.0"
  },
  "peerDependencies": {
    "rxjs": "5.5.x",
    "winston": "2.4.x"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "./node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts)$",
    "moduleFileExtensions": ["ts", "js"]
  },
  "engines": {
    "node": ">=8.9"
  },
  "files": ["lib", "package.json", "README.md", "CHANGELOG.md", "LICENCE"],
  "repository": {
    "type": "git",
    "url": "https://github.com/kevinoneill/wee-events.git"
  },
  "publishConfig": {
    "access": "public"
  }
}
