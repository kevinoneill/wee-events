{
  "name": "@weegigs/events-mongo",
  "version": "0.12.3",
  "description": "Mongo event store and projections for @weegigs/events",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "author": "Kevin O'Neill <kevin@oneill.id.au>",
  "scripts": {
    "build": "yarn run clean && yarn run lint && yarn run compile",
    "clean": "rimraf lib",
    "compile:tsc": "tsc -p .",
    "compile": "yarn run compile:tsc",
    "lint": "tslint -c tslint.json --project . \"src/**/*.ts\"",
    "test:unit": "jest \"tests/*\" --env=node --coverage",
    "test": "yarn run test:unit",
    "watch:tsc": "tsc -p . --watch",
    "watch:test": "jest \"tests/*\" --env=node --coverage --watch",
    "watch": "yarn run watch:tsc & yarn run watch:test",
    "prepublishOnly": "yarn run clean && yarn run lint && yarn run compile"
  },
  "devDependencies": {
    "@types/dotenv": "4.0.3",
    "@types/faker": "4.1.2",
    "@types/jest": "22.2.3",
    "@types/lodash": "4.14.108",
    "@types/mongodb": "3.0.18",
    "@types/mz": "0.0.32",
    "@types/node": "10.0.9",
    "@types/ramda": "0.25.28",
    "@types/winston": "2.3.9",
    "@weegigs/events-core": "0.11.0",
    "concurrently": "3.5.1",
    "dotenv": "5.0.1",
    "faker": "4.1.0",
    "jest": "22.4.3",
    "jest-cli": "22.4.3",
    "jest-environment-node-debug": "2.0.0",
    "prettier": "1.12.1",
    "ramda": "0.25.0",
    "rimraf": "2.6.2",
    "rxjs": "6.1.0",
    "rxjs-tslint": "^0.1.3",
    "ts-jest": "22.4.6",
    "tslint": "5.10.0",
    "tslint-config-prettier": "1.12.0",
    "tslint-config-standard": "7.0.0",
    "typescript": "2.8.3",
    "winston": "2.4.2"
  },
  "dependencies": {
    "@weegigs/concurrent": "1.0.0",
    "lodash": "4.17.10",
    "mongodb": "3.0.5"
  },
  "peerDependencies": {
    "@weegigs/events-core": "0.9.1",
    "rxjs": "^6.1",
    "winston": "^2.4"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "./node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts)$",
    "moduleFileExtensions": ["ts", "js", "json"],
    "mapCoverage": true
  },
  "engines": {
    "node": ">=8.9"
  },
  "files": ["lib", "package.json", "README.md", "CHANGELOG.md", "LICENCE"],
  "repository": {
    "type": "git",
    "url": "https://github.com/kevinoneill/wee-events.git"
  },
  "publishConfig": {
    "access": "public"
  }
}
